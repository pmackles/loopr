<% current_channel "courses" %>
<% current_tab "rounds" %>
<% title @course.name %>
	
<div style="width: 375px">
	<div style="padding:0.3em 0">
		<div style="float: right;">
			<%= will_paginate @rounds, :page_links => false %>
		</div>
		<div style="padding:0.3em 0">
			<%= counts @rounds %>
		</div>
	</div>
	<%= render :partial => 'rounds/list', :object => @rounds %>
</div>
