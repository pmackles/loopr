<% title username(@user, :possessive => true) + " Game: Rounds" %>
<% current_tab "rounds" %>

<% if @round_table.rounds.any? %>
	<%= render :partial => 'rounds/round_table', :object => @round_table %>
<% else %>
	No Rounds Yet.
	<% if same_as_current_user?(@user) %>
		What are you waiting for? Start <%= link_to "posting scores", round_new_url() %> already!
	<% end %>
<% end %>
